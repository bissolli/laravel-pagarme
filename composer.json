{
    "name": "bissolli/laravel-pagarme",
    "description": "Laravel 5 package for Pagar.me library",
    "keywords": [ "pagarme", "gateway", "payment", "laravel", "checkout", "intermediary", "pagar.me"],
    "minimum-stability": "stable",
    "license": "MIT",
    "homepage": "https://github.com/bissolli/laravel-pagarme",
    "authors": [
        {
            "name": "Gustavo Bissolli",
            "email": "gustavo.bissolli@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/config": "5.*",
        "illuminate/support": "5.*",
        "pagarme/pagarme-php": "^1.1",
        "mockery/mockery": "^0.9.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Bissolli\\LaravelPagarme\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bissolli\\LaravelPagarme\\Tests\\": "tests/"
        }
    }
}
